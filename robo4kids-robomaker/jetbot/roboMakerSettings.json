{
  "runConfigurations": [
    {
      "id": "JetBot_Cfg01",
      "name": "JetBot Simulation",
      "type": "colcon build",
      "cfg": {
        "workingDir": "./robo4kids/robo4kids-robomaker/jetbot/simulation_ws",
        "cmdArgs": ""
      }
    },
    {
      "id": "JetBot_Cfg02",
      "name": "JetBot Simulation",
      "type": "colcon bundle",
      "cfg": {
        "workingDir": "./robo4kids/robo4kids-robomaker/jetbot/simulation_ws",
        "cmdArgs": ""
      }
    },
    {
      "id": "JetBot_SimulationJob",
      "name": "JetBot Teleop Simulation",
      "type": "simulation",
      "cfg": {
          "simulationApp": {
            "name": "jetbotTeleopSimulation",
            "sourceBundleFile": "./robo4kids/robo4kids-robomaker/jetbot/simulation_ws/bundle/output.tar",
            "architecture": "X86_64",
            "s3Bucket": "<Update S3 Bucketname Here>",
            "launchConfig": {
              "packageName": "jetbot_sim_app",
              "launchFile": "teleop.launch",
              "environmentVariables": {
                "IOT_ENDPOINT": "<Update IoT Endpoint Here>"
              }
            },
            "simulationSoftwareSuite": {
              "name": "Gazebo",
              "version": "9"
            },
            "renderingEngine": {
              "name": "OGRE",
              "version": "1.x"
            },
            "robotSoftwareSuite": {
              "name": "ROS",
              "version": "Melodic"
            }
          },
          "simulation": {
            "maxJobDurationInSeconds": 28800,
            "failureBehavior": "Fail",
            "iamRole": "<Update IAM Role ARN Here>",
            "outputLocation": "<Update S3 Bucketname Here>",
            "vpcConfig": {
              "subnets": [
                "<Update Public Subnet 1 Here>",
                "<Update Public Subnet 2 Here>"
              ],
              "securityGroups": [
                "<Update Security Group Here>"
              ],
              "assignPublicIp": true
            },
            "tags": {}
          }
      }
    },
    {
      "id": "JetBot_wf1",
      "type": "workflow",
      "name": "JetBot Simulation - Build and Bundle",
      "runCfgIds": [
        "JetBot_Cfg01",
        "JetBot_Cfg02"
      ]
    }
  ]
}